          "title": "Type",
          "description": "type",
          "$id": "/properties/type",
          "type": "string",
          "pattern": "^played$",
          "default": "played",
          "examples": [
            "played"
          ]
        },
        "lang": {
          "title": "Language",
          "description": "language code; ISO 639-1:2002, Codes for the representation of names of languages â€” Part 1: Alpha-2 code",
          "$id": "/definitions/village/properties/lang",
          "type": "string",
          "minLength": 2,
          "maxLength": 2,
          "default": "en",
          "examples": [
            "en",
            "ja",
            "fr",
            "it"
          ]
        }
      },
      "required": [
