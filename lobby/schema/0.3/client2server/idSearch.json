{
  "title": "werewolf.world: Schema of idSearch.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://werewolf.world/lobby/schema/0.3/client2server/idSearch.json",
  "definitions": {},
  "type": "object",
  "properties": {
    "type": {
      "title": "Type",
      "description": "type",
      "$id": "/properties/type",
      "type": "string",
      "pattern": "^idSearch$",
      "default": "idSearch",
      "examples": [
        "idSearch"
      ]
    },
    "token": {
      "title": "User Token",
      "description": "GUID",
      "$id": "/properties/token",
      "type": "string",
      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
      "examples": [
        "3F2504E0-4F89-11D3-9A0C-0305E82C3301"
      ]
    },
    "lobby": {
      "title": "Lobby Type",
      "description": "lobby type",
      "$id": "/properties/lobby",
      "type": "string",
      "enum": [
        "human player",
        "robot player",
        "onymous audience",
        "anonymous audience"
      ],
      "examples": [
        "human player",
        "robot player",
        "onymous audience",
        "anonymous audience"
      ]
    },
    "idForSearching": {
      "title": "ID for Searching",
      "description": "ID for searching",
      "$id": "/properties/idForSearching",
      "type": "integer",
      "minimum": 0,
      "maximum": 999,
      "examples": [
        123
      ]
    }
  },
  "required": [
    "type",
    "token",
    "lobby",
    "idForSearching"
  ]
}
