{
  "title": "werewolf.world: Schema of enterLobby.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://werewolf.world/example/0.2/lobby/schema/client2server/enterLobby.json",
  "definitions": {},
  "type": "object",
  "properties": {
    "type": {
      "title": "Type",
      "description": "type",
      "$id": "/properties/type",
      "type": "string",
      "pattern": "^enterLobby$",
      "default": "enterLobby",
      "examples": [
        "enterLobby"
      ]
    },
    "token": {
      "title": "User Token",
      "description": "GUID",
      "$id": "/properties/token",
      "type": "string",
      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
      "examples": [
        "3F2504E0-4F89-11D3-9A0C-0305E82C3301"
      ]
    },
    "lobby": {
      "title": "Lobby Type",
      "description": "Lobby for human player, robot player or onymous audience?",
      "$id": "/properties/lobby",
      "type": "string",
      "enum": [
        "human player",
        "robot player",
        "onymous audience",
        "anonymous audience"        
      ],
      "examples": [
        "human player",
        "robot player",
        "onymous audience",
        "anonymous audience"
      ]
    },
    "page": {
      "title": "Pagenation",
      "description": "page number",
      "$id": "/properties/page",
      "type": "integer",
      "minimum": 1,
      "default": 1,
      "examples": [
        1
      ]
    }
  },
  "required": [
    "token",
    "lobby",
    "page"
  ]
}
