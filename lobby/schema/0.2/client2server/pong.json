{
  "title": "werewolf.world: Schema of pong.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://werewolf.world/example/0,2/lobby/schema/client2server/pong.json",
  "definitions": {},
  "type": "object",
  "properties": {
    "type": {
      "title": "Type",
      "description": "type",
      "$id": "/properties/type",
      "type": "string",
      "pattern": "^pong$",
      "default": "pong",
      "examples": [
        "pong"
      ]
    },
    "token": {
      "title": "User Token",
      "description": "User ID (GUID)",
      "$id": "/properties/token",
      "type": "string",
      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
      "examples": [
        "3F2504E0-4F89-11D3-9A0C-0305E82C3301"
      ]
    },
    "id": {
      "title": "Ping-pong Token",
      "description": "Ping-pong ID (GUID)",
      "$id": "/properties/id",
      "type": "string",
      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
      "examples": [
        "3F2504E0-4F89-11D3-9A0C-0305E82C3300"
      ]
    }
  },
  "required": [
    "type",
    "token",
    "id"
  ]
}
