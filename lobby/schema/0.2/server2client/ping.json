{
  "title": "werewolf.world: Schema of ping.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://werewolf.world/example/0.2/lobby/schema/server2client/ping.json",
  "definitions": {},
  "type": "object",
  "properties": {
    "type": {
      "title": "Type",
      "description": "type",
      "$id": "/properties/type",
      "type": "string",
      "pattern": "^ping$",
      "default": "ping",
      "examples": [
        "ping"
      ]
    },
    "id": {
      "title": "Ping-pong Token",
      "description": "Ping-pong ID (GUID)",
      "$id": "/properties/id",
      "type": "string",
      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
      "examples": [
        "3F2504E0-4F89-11D3-9A0C-0305E82C3300"
      ]
    },
    "results": {
      "title": "Results",
      "description": "results",
      "$id": "/properties/results",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "token": {
            "title": "User Token",
            "description": "User ID (GUID)",
            "$id": "/properties/results/items/properties/token",
            "type": "string",
            "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
            "examples": [
              "3F2504E0-4F89-11D3-9A0C-0305E82C3301"
            ]
          },
          "ping": {
            "title": "Ping",
            "description": "ping",
            "$id": "/properties/results/items/properties/ping",
            "pattern": "^[0-9]{2}.[0-9]{3} s$",
            "type": "string",
            "default": "99.999 s",
            "examples": [
              "99.999 s"
            ]
          }
        },
        "required": [
          "token",
          "ping"
        ]
      }
    }
  },
  "required": [
    "type",
    "id",
    "results"
  ]
}
