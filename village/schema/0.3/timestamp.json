{
    "title": "werewolf.world: Schema of timestamp.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://werewolf.world/village/schema/0.3/timestamp.json",
    "definitions": {
        "phaseStartTime": {
            "title": "Timestamp",
            "description": "timestamp",
            "$id": "#timestamp",
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}.[0-9]{3}(\\+[0-9]{2}:[0-9]{2}|Z)$",
            "examples": [
                "2006-10-07T12:06:56.568+09:00"
            ]
        }
    }
}