{
	"@context": {
		"@version": 1.1,
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.2/",
		"role": "w3:role",
		"name": {
			"@id": "w3:roleName",
			"@container": "@language"
		},
		"image": "w3:roleImage",
		"isMine": {
			"@id": "w3:roleIsMine",
			"@type": "xsd:boolean"
		},
		"numberOfAgents": {
			"@id": "w3:numberOfAgentsWhoPlayTheRole",
			"@type": "xsd:integer"
		},
		"agent": "w3:agent",
		"board": "w3:board"
	}
}