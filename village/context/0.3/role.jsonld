{
	"@context": {
		"@version": 1.1,
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.3/",
		"role": "w3:role",
		"name": {
			"@id": "w3:roleName",
			"@container": "@language"
		},
		"image": {
			"@id": "w3:roleImage",
			"@type": "xsd:anyURI"
		},
		"isMine": {
			"@id": "w3:roleIsMine",
			"@type": "xsd:boolean"
		},
		"numberOfPlayers": {
			"@id": "w3:numberOfPlayersWhoPlayTheRole",
			"@type": "xsd:unsignedShort"
		},
		"character": "w3:character",
		"board": "w3:board"
	}
}
