{
	"@context": {
		"@version": 1.1,
		"xsd": "https://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.3/",
		"village": "w3:village",
		"token": {
			"@id": "w3:token",
			"@type": "xsd:string"
		},
		"phase": {
			"@id": "w3:phase",
			"@type": "xsd:string"
		},
		"day": {
			"@id": "w3:day",
			"@type": "xsd:unsignedShort"
		},
		"phaseTimeLimit": {
			"@id": "w3:phaseTimeLimit",
			"@type": "xsd:unsignedShort"
		},
		"phaseStartTime": {
			"@id": "w3:phaseStartTime",
			"@type": "xsd:dateTime"
		},
		"serverTimestamp": {
			"@id": "w3:timestamp",
			"@type": "xsd:dateTime"
		},
		"clientTimestamp": {
			"@id": "w3:timestamp",
			"@type": "xsd:dateTime"
		},
		"directionality": {
			"@id": "w3:directionality",
			"@type": "xsd:string"
		},
		"intensionalDisclosureRange": {
			"@id": "w3:channel",
			"@type": "xsd:string"
		},
		"extensionalDisclosureRange": "w3:extensionalDisclosureRange",
		"avatar": "w3:avatar",
		"myCharacter": "w3:character",
		"flavorText": {
			"@id": "w3:flavorText",
			"@container": "@list"
		}
	}
}
