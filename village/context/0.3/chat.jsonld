{
	"@context": {
		"@version": 1.1,
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.3/",
		"isMine": {
			"@id": "w3:chatIsMine",
			"@type": "xsd:boolean"
		},
		"character": "w3:character",
		"id": {
			"@id": "w3:chatId",
			"@type": "xsd:integer"
		},
		"counter": {
			"@id": "w3:chatCounter",
			"@type": "xsd:integer"
		},
		"maxNumberOfChatMessages": {
			"@id": "w3:chatMaxNumberOfChatMessages",
			"@type": "xsd:integer"
		},
		"interval": {
			"@id": "w3:chatInterval",
			"@type": "xsd:integer"
		},
		"text": {
			"@id": "w3:chatText",
			"@type": "xsd:string"
		},
		"maxLengthOfUnicodeCodePoints": {
			"@id": "w3:chatMaxLengthOfUnicodeCodePoints",
			"@type": "xsd:integer"
		},
		"isOver": {
			"@id": "w3:chatIsOver",
			"@type": "xsd:boolean"
		},
		"isFromServer": {
			"@id": "w3:chatIsFromServer",
			"@type": "xsd:boolean"
		}
	}
}
