{
	"@context": {
		"@version": 1.1,
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.3/",
		"character": "w3:character",
		"isMine": {
			"@id": "w3:characterIsMine",
			"@type": "xsd:boolean"
		},
		"name": {
			"@id": "w3:characterName",
			"@container": "@language"
		},
		"image": {
			"@id": "w3:characterImage",
			"@type": "xsd:anyURI"
		},
		"id": {
			"@id": "w3:characterId",
			"@type": "xsd:unsignedShort"
		},
		"role": "w3:role",
		"status": {
			"@id": "w3:characterStatus",
			"@type": "xsd:string"
		},
		"update": "w3:update",
		"isAChoice": {
			"@id": "w3:characterIsAChoice",
			"@type": "xsd:boolean"
		},
		"result": {
			"@id": "w3:result",
			"@type": "xsd:string"
		},
		"avatar": "w3:avatar",
		"fullName": {
			"@id": "w3:characterFullName",
			"@container": "@language"
		},
		"gender": {
			"@id": "w3:gender",
			"@type": "xsd:string"
		}
	}
}
