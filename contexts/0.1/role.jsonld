{
	"@context": {
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "http://werewolf.world/resource/0.1/",
		"role": "w3:role",
		"name": {
			"@id": "w3:roleName",
			"@container": "@language"
		},
		"image": "w3:roleImage",
		"roleIsMine": {
			"@id": "w3:roleIsMine",
			"@type": "xsd:boolean"
		},
		"number": {
			"@id": "w3:roleNumber",
			"@type": "xsd:integer"
		},
		"agentId": {
			"@id": "w3:agentId",
			"@type": "xsd:integer"
		},
		"board": {
			"@id": "w3:board",
			"boardPolarity": "w3:boardPolarity",
			"boardAgentId": {
				"@id": "w3:agentId",
				"@type": "xsd:integer"
			},
			"boardPhase": "w3:phase",
			"boardDay": {
				"@id": "w3:boardDay",
				"@type": "xsd:integer"
			}
		}
	}
}
