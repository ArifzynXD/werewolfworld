{
	"@context": {
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.1/",
		"role": {
			"@id": "w3:role",
			"@type": "@id"
		},
		"name": {
			"@id": "w3:roleName",
			"@container": "@language"
		},
		"image": "w3:roleImage",
		"roleIsMine": {
			"@id": "w3:roleIsMine",
			"@type": "xsd:boolean"
		},
		"numberOfAgents": {
			"@id": "w3:numberOfAgentsWhoPlayTheRole",
			"@type": "xsd:integer"
		},
		"agent": {
			"@id": "w3:agent",
			"@type": "@id",
			"agentId": {
				"@id": "w3:agentId",
				"@type": "xsd:integer"
			},
			"agentName": {
				"@id": "w3:agentName",
				"@container": "@language"
			},
			"agentImage": {
				"@id": "w3:agentImage",
				"@type": "@id"
			}
		},
		"board": {
			"@id": "w3:board",
			"boardPolarity": "w3:boardPolarity",
			"boardAgent": {
				"@id": "w3:agent",
				"boardAgentId": {
					"@id": "w3:agentId",
					"@type": "xsd:integer"
				},
				"boardAgentName": {
					"@id": "w3:agentName",
					"@container": "@language"
				},
				"boardAgentImage": {
					"@id": "w3:agentImage",
					"@type": "@id"
				}
			},
			"boardPhase": "w3:phase",
			"boardDate": {
				"@id": "w3:date",
				"@type": "xsd:integer"
			}
		}
	}
}
