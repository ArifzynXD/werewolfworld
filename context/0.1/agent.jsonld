{
	"@context": {
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "http://werewolf.world/resource/0.1/",
		"agent": "w3:agent",
		"agentIsMine": {
			"@id": "w3:agentIsMine",
			"@type": "xsd:boolean"
		},
		"name": {
			"@id": "w3:agentName",
			"@container": "@language"
		},
		"image": "w3:agentImage",
		"id": {
			"@id": "w3:agentId",
			"@type": "xsd:integer"
		},
		"role": {
			"@id": "w3:role",
			"roleName": {
				"@id": "w3:roleName",
				"@container": "@language"
			},
			"roleImage": "w3:roleImage"
		},
		"status": "w3:agentStatus",
		"statusUpdatePhase": "w3:phase",
		"statusUpdateDate": "w3:date",
		"result": "w3:result",
		"userName": "w3:userName",
		"userAvatar": "w3:userAvatar"
	}
}
