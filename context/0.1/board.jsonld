{
	"@context": {
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.1/",
		"boardAgent": {
			"@id": "w3:agent",
			"boardAgentId": {
				"@id": "w3:agentId",
				"@type": "xsd:integer"
			},
			"boardAgentName": {
				"@id": "w3:agentId",
				"@container": "@language"
			},
			"boardAgentImage": {
				"@id": "w3:agentImage",
				"@type": "@id"
			}
		},
		"boardRole": "w3:role",
		"boardPrediction": "w3:boardPrediction"
	}
}
