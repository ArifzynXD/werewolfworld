{
	"@context": {
		"@version": 1.1,
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.2/",
		"isMine": {
			"@id": "w3:chatIsMine",
			"@type": "xsd:boolean"
		},
		"agent": {
			"@id": "w3:agent",
			"@type": "@id",
			"id": {
				"@id": "w3:agentId",
				"@type": "xsd:integer"
			},
			"name": {
				"@id": "w3:agentName",
				"@container": "@language"
			},
			"image": {
				"@id": "w3:agentImage",
				"@type": "@id"
			}
		},
		"user": {
			"@id": "w3:user",
			"@type": "@id",
			"name": "w3:userName",
			"avatar": {
				"@id": "w3:userAvatar",
				"@type": "@id"
			}
			
		},
		"id": "w3:chatId",
		"counter": "w3:chatCounter",
		"limit": "w3:chatLimit",
		"interval": "w3:chatInterval",
		"text": "w3:chatText",
		"characterLimit": "w3:chatCharacterLimit",
		"isOver": "w3:chatIsOver"
	}
}
