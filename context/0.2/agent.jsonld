{
	"@context": {
		"@version": 1.1,
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.2/",
		"agent": "w3:agent",
		"isMine": {
			"@id": "w3:agentIsMine",
			"@type": "xsd:boolean"
		},
		"name": {
			"@id": "w3:agentName",
			"@container": "@language"
		},
		"image": "w3:agentImage",
		"id": {
			"@id": "w3:agentId",
			"@type": "xsd:integer"
		},
		"role": "w3:role",
		"status": "w3:agentStatus",
		"update": {
			"phase": "w3:phase",
			"date": {
				"@id": "w3:date",
				"@type": "xsd:integer"
			}
		},
		"isAChoice": {
			"@id": "w3:agentIsAChoice",
			"@type": "xsd:boolean"
		},
		"result": "w3:result",
		"avatar": {
			"@id": "w3:avatar",
			"@type": "@id",
			"@context": "https://werewolf.world/context/0.2/avatar.jsonld"
		}
	}
}