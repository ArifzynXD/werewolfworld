{
	"@context": {
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.2/",
		"role": {
			"@id": "w3:role",
			"@type": "@id"
		},
		"name": {
			"@id": "w3:roleName",
			"@container": "@language"
		},
		"image": {
			"@id": "w3:roleImage",
			"@type": "@id"
		},
		"isMine": {
			"@id": "w3:roleIsMine",
			"@type": "xsd:boolean"
		},
		"numberOfAgents": {
			"@id": "w3:numberOfAgentsWhoPlayTheRole",
			"@type": "xsd:integer"
		},
		"agent": {
			"@id": "w3:agent",
			"@type": "@id",
			"id": {
				"@id": "w3:agentId",
				"@type": "xsd:integer"
			},
			"name": {
				"@id": "w3:agentName",
				"@container": "@language"
			},
			"image": {
				"@id": "w3:agentImage",
				"@type": "@id"
			}
		},
		"board": {
			"@id": "w3:board",
			"polarity": "w3:boardPolarity",
			"agent": {
				"@id": "w3:agent",
				"@type": "@id",
				"id": {
					"@id": "w3:agentId",
					"@type": "xsd:integer"
				},
				"name": {
					"@id": "w3:agentName",
					"@container": "@language"
				},
				"image": {
					"@id": "w3:agentImage",
					"@type": "@id"
				}
			},
			"phase": "w3:phase",
			"date": {
				"@id": "w3:date",
				"@type": "xsd:integer"
			}
		}
	}
}
