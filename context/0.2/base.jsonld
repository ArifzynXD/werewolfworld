{
	"@context": {
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.2/",
		"village": {
			"@id": "w3:village",
			"id": {
				"@id": "w3:villageId",
				"@type": "xsd:integer"
			},
			"name": "w3:villageName",
			"totalNumberOfAgents": {
				"@id": "w3:totalNumberOfAgents",
				"@type": "xsd:integer"
			}
		},
		"token": "w3:token",
		"phase": "w3:phase",
		"date": {
			"@id": "w3:date",
			"@type": "xsd:integer"
		},
		"phaseTimeLimit": {
			"@id": "w3:phaseTimeLimit",
			"@type": "xsd:integer"
		},
		"phaseStartTime": {
			"@id": "w3:phaseStartTime",
			"@type": "xsd:dateTime"
		},
		"serverTimestamp": {
			"@id": "w3:timestamp",
			"@type": "xsd:dateTime"
		},
		"clientTimestamp": {
			"@id": "w3:timestamp",
			"@type": "xsd:dateTime"
		},
		"directionality": "w3:directionality",
		"disclosureRange": {
			"@id": "w3:disclosureRange",
			"intensional": "w3:channel",
			"extensional": {
				"@id": "w3:agent",
				"@type": "@id",
				"id": {
					"@id": "w3:agentId",
					"@type": "xsd:integer"
				},
				"name": {
					"@id": "w3:agentName",
					"@container": "@language"
				},
				"image": {
					"@id": "w3:agentImage",
					"@type": "@id"
				},
				"role": {
					"@id": "w3:role",
					"@type": "@id",
					"name": {
						"@id": "w3:roleName",
						"@container": "@language"
					},
					"image": {
						"@id": "w3:roleImage",
						"@type": "@id"
					}
				},
				"status": "w3:agentStatus"
			}
		},
		"me": {
			"@id": "w3:user",
			"name": "w3:userName",
			"avatar": {
				"@id": "w3:userAvatar",
				"@type": "@id"
			}
		},
		"myAgent": {
			"@id": "w3:agent",
			"@type": "@id",
			"id": {
				"@id": "w3:agentId",
				"@type": "xsd:integer"
			},
			"name": {
				"@id": "w3:agentName",
				"@container": "@language"
			},
			"image": {
				"@id": "w3:agentImage",
				"@type": "@id"
			},
			"role": {
				"@id": "w3:role",
				"@type": "@id",
				"name": {
					"@id": "w3:roleName",
					"@container": "@language"
				},
				"image": {
					"@id": "@w3:roleImage",
					"@type": "@id"
				}
			}
		}
	}
}
