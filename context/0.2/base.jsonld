{
	"@context": {
		"@version": 1.1,
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"w3": "https://werewolf.world/resource/0.2/",
		"w3c": "https://werewolf.world/context/0.2/",
		"village": "w3:village",
		"token": "w3:token",
		"phase": "w3:phase",
		"date": {
			"@id": "w3:date",
			"@type": "xsd:integer"
		},
		"phaseTimeLimit": {
			"@id": "w3:phaseTimeLimit",
			"@type": "xsd:integer"
		},
		"phaseStartTime": {
			"@id": "w3:phaseStartTime",
			"@type": "xsd:dateTime"
		},
		"serverTimestamp": {
			"@id": "w3:timestamp",
			"@type": "xsd:dateTime"
		},
		"clientTimestamp": {
			"@id": "w3:timestamp",
			"@type": "xsd:dateTime"
		},
		"directionality": "w3:directionality",
		"intensionalDisclosureRange": "w3:channel",
		"extensionalDisclosureRange": "w3:extensionalDisclosureRange",
		"avatar": {
			"@id": "w3:avatar",
			"@type": "@id",
			"@context": "w3c:avatar.jsonld"
		},
		"myAgent": {
			"@id": "w3:agent",
			"@type": "@id",
			"id": {
				"@id": "w3:agentId",
				"@type": "xsd:integer"
			},
			"name": {
				"@id": "w3:agentName",
				"@container": "@language"
			},
			"image": {
				"@id": "w3:agentImage",
				"@type": "@id"
			},
			"role": {
				"@id": "w3:role",
				"@type": "@id",
				"name": {
					"@id": "w3:roleName",
					"@container": "@language"
				},
				"image": {
					"@id": "@w3:roleImage",
					"@type": "@id"
				}
			}
		}
	}
}
